generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entity {
  entity_id Int    @id @default(autoincrement())
  name      String @db.VarChar(255)
}

model InfoProvider {
  id      Int @id @default(autoincrement())
  kind    String @db.VarChar(50)
}